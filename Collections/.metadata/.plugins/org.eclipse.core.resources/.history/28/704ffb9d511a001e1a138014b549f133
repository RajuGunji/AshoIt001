package PriorityQueue;
import java.util.*;
public class Poll_or_Remove 
{
	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		Queue<Object> q=new PriorityQueue<>();
		q.offer(1);
		q.offer(6);
		q.offer(9);
		q.offer(3);
		q.offer(7);
		q.offer(2);
			System.out.println(q);
			System.out.println(q.peek());
			q.poll();			
				System.out.println(q);		 	
				q.remove();
				System.out.println(q);	
	}
}
