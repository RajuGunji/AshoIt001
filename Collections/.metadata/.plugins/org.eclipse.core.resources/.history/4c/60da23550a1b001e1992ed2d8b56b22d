package List;
import java.util.*;
public class Arrays_dot_toString
{
	public static void main(String[] args)
	{
		List<String> l=new ArrayList<>();
		l.add("Sree");
		l.add("Ram");
		l.add("Jai");
		l.add("Hanuman");
		String[] arr=(String[]) l.toArray();
		System.out.println(arr.toString());
	}

}
